                {% if mail.footerBannerImg | trans %}
                    <table align="center" class="container banner float-center" style="Margin:0 auto;background:#fefefe;border-collapse:collapse;border-spacing:0;float:none;margin:0 auto;padding:0;text-align:center;vertical-align:top;width:580px">
