                                {% endif %}
                                <img src="{{ mail.footerBannerImg | trans }}" alt="{{ mail.footerBannerAlt | trans }}" style="-ms-interpolation-mode:bicubic;border:none;clear:both;display:block;max-width:100%;outline:0;text-decoration:none;width:auto">
                                {% if mail.footerBannerLink | trans %}
