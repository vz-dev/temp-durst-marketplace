<?xml version="1.0"?>
<database xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="zed" xsi:noNamespaceSchemaLocation="http://static.spryker.com/schema-01.xsd" namespace="Orm\Zed\Sales\Persistence" package="src.Orm.Zed.Sales.Persistence">

    <table name="spy_sales_expense">
        <column name="quantity" type="INTEGER" required="false" default="1" />
        <column name="is_negative" type="BOOLEAN" required="true" default="false" />
        <column name="merchant_sku" size="255" type="VARCHAR" required="false" />
    </table>

    <table name="spy_sales_order_totals" phpName="SpySalesOrderTotals">
        <column name="deposit_total" type="INTEGER" required="false" default="0" />
        <column name="display_total" type="INTEGER" required="false" default="0" />
        <column name="gross_subtotal" type="INTEGER" required="false" default="0" />
        <column name="weight_total" type="INTEGER" required="false" default="0" />
    </table>

</database>
