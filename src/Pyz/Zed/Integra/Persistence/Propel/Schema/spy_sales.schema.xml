<?xml version="1.0"?>
<database xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="zed" xsi:noNamespaceSchemaLocation="http://static.spryker.com/schema-01.xsd" namespace="Orm\Zed\Sales\Persistence" package="src.Orm.Zed.Sales.Persistence">

    <table name="spy_sales_order" phpName="SpySalesOrder">
        <column name="integra_customer_no" required="false" size="255" type="VARCHAR"/>
        <column name="integra_receipt_did" required="false" size="255" type="VARCHAR"/>
        <column name="integra_receipt_no" required="false" size="255" type="VARCHAR"/>
        <column name="is_external" required="false" type="BOOLEAN" defaultValue="false"/>
        <column name="is_exportable" required="true" defaultValue="false" type="BOOLEAN"/>
        <column name="is_closable" required="true" defaultValue="false" type="BOOLEAN"/>
        <column name="integra_payment_method" required="false" type="INTEGER"/>
    </table>

    <table name="spy_sales_order_item" phpName="SpySalesOrderItem">
        <column name="integra_position_did" required="false" size="255" type="VARCHAR"/>
    </table>

</database>
