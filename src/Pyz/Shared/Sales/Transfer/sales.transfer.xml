<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd" >

    <transfer name="OrderList">
        <property name="pagination" type="Pagination" />
    </transfer>

    <transfer name="Pagination">
        <property name="page" type="int" />
        <property name="maxPerPage" type="int" />
        <property name="nbResults" type="int" />
        <property name="previousPage" type="int" />
        <property name="nextPage" type="int" />
        <property name="firstIndex" type="int" />
        <property name="lastIndex" type="int" />
        <property name="firstPage" type="int" />
        <property name="lastPage" type="int" />
    </transfer>

    <transfer name="Comment">
        <property name="type" type="string" />
    </transfer>

    <transfer name="Order">
        <property name="signatureFilePath" type="string"/>
        <property name="signedAt" type="int"/>
        <property name="deliveryOrder" type="int"/>
        <property name="customerNote" type="string"/>
        <property name="fkDriver" type="int"/>
        <property name="isReseller" type="bool"/>
        <property name="platform" type="string"/>
        <property name="version" type="string"/>
        <property name="deviceType" type="string"/>
        <property name="externalAmountPaid" type="int"/>
        <property name="durstCustomerReference" type="bool"/>
        <property name="fkGraphmastersTour" type="int"/>
        <property name="graphmastersStopEta" type="string"/>
        <property name="graphmastersStatus" type="string"/>
    </transfer>

    <transfer name="Address">
        <property name="lat" type="float"/>
        <property name="lng" type="float"/>
        <property name="floor" type="int"/>
        <property name="elevator" type="bool"/>
    </transfer>

    <transfer name="Item">
        <property name="deliveryStatus" type="string"/>
    </transfer>

</transfers>
