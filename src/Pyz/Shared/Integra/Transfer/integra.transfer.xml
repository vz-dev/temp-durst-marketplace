<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="IntegraCredentials">
        <property name="idIntegraCredentials" type="int"/>
        <property name="fkBranch" type="int"/>
        <property name="useIntegra" type="bool"/>
        <property name="integraIpAddress" type="string"/>
        <property name="soapAuthUser" type="string"/>
        <property name="soapAuthPassword" type="string"/>
        <property name="soapAuthMandant" type="int"/>
        <property name="soapAuthBetrStr" type="int"/>
        <property name="ftpHost" type="string"/>
        <property name="ftpUser" type="string"/>
        <property name="ftpPassword" type="string"/>
        <property name="openOrderCsvPath" type="string"/>
        <property name="closedOrderCsvPath" type="string"/>
    </transfer>

    <transfer name="Order">
        <property name="isExportable" type="bool"/>
        <property name="isClosable" type="bool"/>
        <property name="integraCustomerNo" type="string"/>
        <property name="integraReceiptDid" type="string"/>
        <property name="integraReceiptNo" type="string"/>
        <property name="isExternal" type="bool"/>
        <property name="integraPaymentMethod" type="int"/>
    </transfer>

    <transfer name="Quote">
        <property name="isExternal" type="bool"/>
        <property name="integraReceiptNo" type="string"/>
        <property name="integraCustomerNo" type="string"/>
        <property name="integraReceiptDid" type="string"/>
        <property name="deliveryOrder" type="int"/>
        <property name="integraPaymentType" type="int"/>
    </transfer>

    <transfer name="Item">
        <property name="integraPositionDid" type="string"/>
    </transfer>

    <transfer name="IntegraWebserviceTokenTransfer">
        <property name="idBranch" type="int"/>
        <property name="token" type="string"/>
        <property name="isValid" type="boolean"/>
    </transfer>

    <transfer name="TimeSlot">
        <property name="integraTourNo" type="string" />
        <property name="integraDeliveryWindowNo" type="string" />
    </transfer>

</transfers>
