<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="Branch">
        <property name="offersDepositPickup" type="bool" />
    </transfer>

    <transfer name="DepositPickupInquiry">
        <property name="idDepositPickupInquiry" type="int" />
        <property name="fkBranch" type="int" />
        <property name="name" type="string" />
        <property name="address" type="string" />
        <property name="email" type="string" />
        <property name="phoneNumber" type="string" />
        <property name="preferredDate" type="string" />
        <property name="message" type="string" />
        <property name="createdAt" type="string" />
    </transfer>

    <transfer name="Mail">
        <property name="idDepositPickupInquiry" type="int" />
    </transfer>

</transfers>
