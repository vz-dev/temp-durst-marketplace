{
    "name": "spryker/docker-sdk-generator",
    "description": "Spryker Docker SDK - Generator",
    "license": "MIT",
    "require": {
        "symfony/yaml": "^4.3",
        "twig/twig": "^2.11",
        "spatie/url": "^1.3",
        "symfony/console": "^5.3",
        "illuminate/collections": "^8.70"
    },
    "autoload": {
        "psr-4": {
            "DeployFileGenerator\\": "deploy-file-generator/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Unit\\": "tests/unit"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "7.3"
        }
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.3"
    }
}
